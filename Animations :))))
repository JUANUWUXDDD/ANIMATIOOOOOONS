IdleAnim.AnimationId = "rbxassetid://17746180844"
IdleAnim.Parent = Folder

-- Animaci贸n de salto
local JumpAnim = Instance.new("Animation")
JumpAnim.Name = "JumpAnim"
JumpAnim.AnimationId = "rbxassetid://13694096724"
